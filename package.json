{
  "name": "a-galaxy-far-far-away",
  "displayName": "A galaxy far, far away",
  "description": "A theme for Visual Studio Code from a galaxy far, far away...",
  "version": "1.0.0",
  "publisher": "DanMad",
  "repository": "https://github.com/DanMad/a-galaxy-far-far-away-theme",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "The Empire",
        "uiTheme": "vs-dark",
        "path": "./themes/empire-theme.json"
      },
      {
        "label": "Rogue Squadron",
        "uiTheme": "vs-dark",
        "path": "./themes/rogue-squadron-theme.json"
      }
    ]
  },
  "devDependencies": {
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
