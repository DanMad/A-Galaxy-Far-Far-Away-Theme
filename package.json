{
  "name": "a-galaxy-far-far-away-theme",
  "displayName": "A Galaxy Far, Far Away Theme",
  "description": "A theme for Visual Studio Code from a galaxy far, far away...",
  "icon": "images/icon.png",
  "author": "DanMad",
  "publisher": "DanMad",
  "license": "MIT",
  "private": true,
  "version": "2.0.0",
  "categories": [
    "Themes",
    "Other"
  ],
  "homepage": "https://github.com/danmad/a-galaxy-far-far-away-theme#readme",
  "galleryBanner": {
    "color": "#06090e",
    "theme": "dark"
  },
  "keywords": [
    "Dark theme",
    "The Empire",
    "Galaxy",
    "Rogue Squadron",
    "Theme"
  ],
  "bugs": {
    "url": "https://github.com/danmad/a-galaxy-far-far-away-theme/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danmad/a-galaxy-far-far-away-theme"
  },
  "scripts": {
    "build": "npx vsce package",
    "deploy": "npx vsce publish"
  },
  "devDependencies": {
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "vsce": "^1.74.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "vscode": "^1.42.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "The Empire",
        "path": "./themes/the-empire-theme.json",
        "uiTheme": "vs-dark"
      },
      {
        "label": "Rogue Squadron",
        "path": "./themes/rogue-squadron-theme.json",
        "uiTheme": "vs-dark"
      }
    ]
  }
}
