{
  "name": "a-galaxy-far-far-away-theme",
  "displayName": "A Galaxy Far, Far Away Theme",
  "version": "3.2.0",
  "private": true,
  "description": "A Visual Studio Code theme from a galaxy far, far away...",
  "categories": [
    "Themes",
    "Other"
  ],
  "keywords": [
    "a11y",
    "Dark Theme",
    "The Empire",
    "Rogue Squadron",
    "Star Wars",
    "Theme",
    "VSCode"
  ],
  "homepage": "https://marketplace.visualstudio.com/items?itemName=DanMad.a-galaxy-far-far-away-theme",
  "bugs": "https://github.com/DanMad/a-galaxy-far-far-away-theme/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanMad/a-galaxy-far-far-away-theme.git"
  },
  "license": "MIT",
  "author": "DanMad",
  "publisher": "DanMad",
  "scripts": {
    "bootstrap": "yarn install && husky install && chmod +x .husky/*",
    "build": "rm -rf themes && mkdir themes && yarn build:the-empire-theme && yarn build:rogue-squadron-theme",
    "build:rogue-squadron-theme": "json-stringify src/rogue-squadron-theme.js -o themes/rogue-squadron-theme.json",
    "build:the-empire-theme": "json-stringify src/the-empire-theme.js -o themes/the-empire-theme.json",
    "publish": "vsce publish"
  },
  "contributes": {
    "themes": [
      {
        "label": "The Empire",
        "path": "./themes/the-empire-theme.json",
        "uiTheme": "vs-dark"
      },
      {
        "label": "Rogue Squadron",
        "path": "./themes/rogue-squadron-theme.json",
        "uiTheme": "vs-dark"
      }
    ]
  },
  "devDependencies": {
    "husky": "^7.0.1",
    "json-stringify-cli": "^1.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.2.1",
    "sort-package-json": "^1.46.0",
    "vsce": "^1.95.1"
  },
  "engines": {
    "vscode": "^1.42.0"
  },
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#04070b",
    "theme": "dark"
  }
}
