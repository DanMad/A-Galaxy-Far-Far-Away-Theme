{
  "name": "The Empire",
  "type": "dark",
  "colors": {
    "activityBar.activeBackground": "#0d121a",
    "activityBar.activeBorder": "#0d121a",
    "activityBar.background": "#010203",
    "activityBar.foreground": "#d1dce7",
    "activityBar.inactiveForeground": "#6c83a0",
    "activityBarBadge.background": "#fa0a0a",
    "activityBarBadge.foreground": "#010203",

    "badge.background": "#fa0a0a",
    "badge.foreground": "#010203",

    "breadcrumb.activeSelectionForeground": "#91a5bc",
    "breadcrumb.focusForeground": "#b1c0d1",
    "breadcrumb.foreground": "#6c83a0",

    "button.background": "#fa0a0a",
    "button.foreground": "#010203",
    "button.hoverBackground": "#fe696b",

    "checkbox.background": "#010203",
    "checkbox.border": "#960505",
    "checkbox.foreground": "#d1dce7",

    "diffEditor.insertedTextBackground": "#787e73",
    "diffEditor.removedTextBackground": "#fa0a0a",

    "dropdown.background": "#010203",
    "dropdown.border": "#960505",
    "dropdown.foreground": "#d1dce7",
    "dropdown.listBackground": "#0d121a",

    "editor.background": "#06090e",
    "editor.findMatchBackground": "#d1dce726",
    "editor.findMatchHighlightBackground": "#d1dce726",
    "editor.foreground": "#b1c0d1",
    "editor.lineHighlightBackground": "#0d121a",
    "editor.selectionBackground": "#d1dce726",
    "editor.wordHighlightBackground": "#d1dce726",
    "editorBracketMatch.background": "#0000",
    "editorBracketMatch.border": "#960505",
    "editorCursor.foreground": "#960505",
    "editorGroup.border": "#0000",
    "editorGroupHeader.tabsBackground": "#010203",
    "editorGutter.background": "#0000",
    "editorGutter.modifiedBackground": "#f4f8fb",
    "editorGutter.addedBackground": "#787e73",
    "editorGutter.deletedBackground": "#fa0a0a",
    "editorLineNumber.foreground": "#6c83a0",
    "editorLineNumber.activeForeground": "#d1dce7",
    "editorPane.background": "#010203",
    "editorWhitespace.foreground": "#f4f8fb1a",
    "editorWidget.background": "#0d121a",
    "editorWidget.border": "#960505",
    "editorWidget.foreground": "#d1dce7",

    "focusBorder": "#960505",
    "foreground": "#d1dce7",

    "gitDecoration.modifiedResourceForeground": "#f4f8fb",
    "gitDecoration.deletedResourceForeground": "#fa0a0a",
    "gitDecoration.untrackedResourceForeground": "#787e73",
    "gitDecoration.ignoredResourceForeground": "#6c83a0",
    "gitDecoration.conflictingResourceForeground": "#960505",

    "input.background": "#010203",
    "input.border": "#0000",
    "input.foreground": "#d1dce7",
    "input.placeholderForeground": "#6c83a0",
    "inputOption.activeBackground": "#0d121a",
    "inputOption.activeBorder": "#960505",

    "panel.border": "#0000",

    "sideBar.background": "#0d121a",
    "sideBar.foreground": "#91a5bc",
    "sideBarSectionHeader.background": "#0d121a",
    "sideBarSectionHeader.foreground": "#b1c0d1",
    "sideBarTitle.foreground": "#d1dce7",

    "statusBar.background": "#0d121a",
    "statusBar.border": "#0000",
    "statusBar.debuggingBackground": "#787e73",
    "statusBar.debuggingForeground": "#010203",
    "statusBar.foreground": "#f4f8fb",
    "statusBar.noFolderBackground": "#fb4649",
    "statusBar.noFolderForeground": "#010203",

    "tab.activeBackground": "#06090e",
    "tab.activeForeground": "#d1dce7",
    "tab.border": "#0000",
    "tab.hoverBackground": "#0d121a",
    "tab.inactiveBackground": "#010203",
    "tab.inactiveForeground": "#91a5bc",

    "terminal.background": "#010203",
    "terminal.border": "#0000",
    "terminal.selectionBackground": "#d1dce726",
    "terminalCursor.foreground": "#960505",

    "textLink.activeForeground": "#fe696b",
    "textLink.foreground": "#fa0a0a",

    "titleBar.border": "#0000",

    "welcomePage.background": "#06090e",

    "window.activeBorder": "#0000",
    "window.inactiveBorder": "#0000"
  },
  "tokenColors": [
    {
      "scope": ["comment.block.css", "comment.block.html", "comment.line"],
      "settings": {
        "foreground": "#6c83a0"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#91a5bc"
      }
    },
    {
      "scope": [
        "constant.language.null",
        "constant.language.undefined",
        "meta.template.expression",
        "punctuation.accessor",
        "punctuation.definition.interpolation",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section",
        "punctuation.separator",
        "support.type.property-name.json"
      ],
      "settings": {
        "fontStyle": "none",
        "foreground": "#b1c0d1"
      }
    },
    {
      "scope": ["constant.language.null", "constant.language.undefined"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "string.other.link.title.markdown",
        "variable.language"
      ],
      "settings": {
        "fontStyle": "none",
        "foreground": "#d1dce7"
      }
    },
    {
      "scope": [
        "markup.quote",
        "keyword.operator.new",
        "storage",
        "support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#f4f8fb"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.name.tag",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "keyword.other.special-method",
        "markup.heading",
        "meta.function-call",
        "support.function",
        "variable.function"
      ],
      "settings": {
        "fontStyle": "none",
        "foreground": "#fa0a0a"
      }
    },
    {
      "scope": ["markup.bold", "markup.italic", "variable"],
      "settings": {
        "foreground": "#fb4649"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic", "variable"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.language", "constant.numeric", "string"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#fe696b"
      }
    },
    {
      "scope": ["constant", "entity.name.type"],
      "settings": {
        "foreground": "#787e73"
      }
    },
    {
      "scope": ["entity.other.inherited-class", "string.regexp"],
      "settings": {
        "fontStyle": "none",
        "foreground": "#99a095"
      }
    },

    {
      "scope": ["constant.other.reference.link.markdown", "keyword.control"],
      "settings": {
        "foreground": "#afb6ab"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f8580c"
      }
    }
  ]
}
