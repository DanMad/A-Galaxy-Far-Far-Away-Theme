{
  "name": "Rogue Squadron",
  "type": "dark",
  "colors": {
    "activityBar.activeBackground": "#0d1219",
    "activityBar.activeBorder": "#0d1219",
    "activityBar.background": "#050709",
    "activityBar.foreground": "#e6e8e9",
    "activityBar.inactiveForeground": "#7b7f81",
    "activityBarBadge.background": "#f8580c",
    "activityBarBadge.foreground": "#050709",

    "badge.background": "#f8580c",
    "badge.foreground": "#050709",

    "breadcrumb.activeSelectionForeground": "#a5a9ab",
    "breadcrumb.focusForeground": "#cbced0",
    "breadcrumb.foreground": "#7b7f81",

    "button.background": "#f8580c",
    "button.foreground": "#050709",
    "button.hoverBackground": "#f78833",

    "checkbox.background": "#050709",
    "checkbox.border": "#f78833",
    "checkbox.foreground": "#e6e8e9",

    "diffEditor.insertedTextBackground": "#538397",
    "diffEditor.removedTextBackground": "#bd615f",

    "dropdown.background": "#050709",
    "dropdown.border": "#f78833",
    "dropdown.foreground": "#e6e8e9",
    "dropdown.listBackground": "#0d1219",

    "editor.background": "#080b0f",
    "editor.findMatchBackground": "#e6e8e926",
    "editor.findMatchHighlightBackground": "#e6e8e926",
    "editor.foreground": "#cbced0",
    "editor.lineHighlightBackground": "#0d1219",
    "editor.selectionBackground": "#e6e8e926",
    "editor.wordHighlightBackground": "#e6e8e926",
    "editorBracketMatch.background": "#0000",
    "editorBracketMatch.border": "#f78833",
    "editorCursor.foreground": "#f78833",
    "editorGroup.border": "#0000",
    "editorGroupHeader.tabsBackground": "#050709",
    "editorGutter.background": "#0000",
    "editorGutter.modifiedBackground": "#b6ad9d",
    "editorGutter.addedBackground": "#538397",
    "editorGutter.deletedBackground": "#bd615f",
    "editorLineNumber.foreground": "#7b7f81",
    "editorLineNumber.activeForeground": "#e6e8e9",
    "editorPane.background": "#050709",
    "editorWhitespace.foreground": "#e6e8e91a",
    "editorWidget.background": "#0d1219",
    "editorWidget.border": "#f78833",
    "editorWidget.foreground": "#e6e8e9",

    "focusBorder": "#f78833",
    "foreground": "#e6e8e9",

    "gitDecoration.modifiedResourceForeground": "#b6ad9d",
    "gitDecoration.deletedResourceForeground": "#bd615f",
    "gitDecoration.untrackedResourceForeground": "#538397",
    "gitDecoration.ignoredResourceForeground": "#7b7f81",
    "gitDecoration.conflictingResourceForeground": "#a34845",

    "input.background": "#050709",
    "input.border": "#0000",
    "input.foreground": "#e6e8e9",
    "input.placeholderForeground": "#7b7f81",
    "inputOption.activeBackground": "#0d1219",
    "inputOption.activeBorder": "#f78833",

    "panel.border": "#0000",

    "sideBar.background": "#0d1219",
    "sideBar.foreground": "#a5a9ab",
    "sideBarSectionHeader.background": "#0d1219",
    "sideBarSectionHeader.foreground": "#cbced0",
    "sideBarTitle.foreground": "#e6e8e9",

    "statusBar.background": "#0d1219",
    "statusBar.border": "#0000",
    "statusBar.debuggingBackground": "#bd615f",
    "statusBar.debuggingForeground": "#050709",
    "statusBar.foreground": "#e6e8e9",
    "statusBar.noFolderBackground": "#f3701a",
    "statusBar.noFolderForeground": "#050709",

    "tab.activeBackground": "#080b0f",
    "tab.activeForeground": "#e6e8e9",
    "tab.border": "#0000",
    "tab.hoverBackground": "#0d1219",
    "tab.inactiveBackground": "#050709",
    "tab.inactiveForeground": "#a5a9ab",

    "terminal.background": "#050709",
    "terminal.border": "#0000",
    "terminal.selectionBackground": "#e6e8e926",
    "terminalCursor.foreground": "#f78833",

    "textLink.activeForeground": "#f78833",
    "textLink.foreground": "#f8580c",

    "titleBar.border": "#0000",

    "welcomePage.background": "#080b0f",

    "window.activeBorder": "#0000",
    "window.inactiveBorder": "#0000"
  },
  "tokenColors": [
    {
      "scope": ["comment.block.css", "comment.block.html", "comment.line"],
      "settings": {
        "foreground": "#7b7f81"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#a5a9ab"
      }
    },
    {
      "scope": [
        "constant.language.null",
        "constant.language.undefined",
        "meta.template.expression",
        "punctuation.accessor",
        "punctuation.definition.interpolation",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section",
        "punctuation.separator",
        "support.type.property-name.json"
      ],
      "settings": {
        "fontStyle": "none",
        "foreground": "#cbced0"
      }
    },
    {
      "scope": ["constant.language.null", "constant.language.undefined"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "string.other.link.title.markdown",
        "variable.language"
      ],
      "settings": {
        "fontStyle": "none",
        "foreground": "#e6e8e9"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#bd615f"
      }
    },
    {
      "scope": [
        "keyword.operator.new",
        "storage",
        "support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#b6ad9d"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.name.tag",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "keyword.other.special-method",
        "markup.heading",
        "meta.function-call",
        "support.function",
        "variable.function"
      ],
      "settings": {
        "fontStyle": "none",
        "foreground": "#f78833"
      }
    },
    {
      "scope": ["markup.bold", "markup.italic", "variable"],
      "settings": {
        "foreground": "#f3701a"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic", "variable"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.language", "constant.numeric", "string"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f8580c"
      }
    },
    {
      "scope": ["constant", "entity.name.type"],
      "settings": {
        "foreground": "#538397"
      }
    },
    {
      "scope": ["entity.other.inherited-class", "string.regexp"],
      "settings": {
        "fontStyle": "none",
        "foreground": "#8fb5c3"
      }
    },
    {
      "scope": "constant.other.reference.link.markdown",
      "settings": {
        "foreground": "#b6ad9d"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#bd615f"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#fa0a0a"
      }
    }
  ]
}
